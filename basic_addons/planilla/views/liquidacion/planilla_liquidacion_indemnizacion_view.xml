
<odoo>

    <record model="ir.ui.view" id="planilla_liquidacion_indemnizacion_view_tree">
        <field name="name">planilla_liquidacion_indemnizacion_view_tree</field>
        <field name="model">planilla.liquidacion.indemnizacion.line</field>
        <field name="arch" type="xml">
            <tree create="0" delete="0" editable="bottom">
                <field name="orden" string="Orden" readonly="1"/>
                <field name="identification_number" readonly="1"/>
                <field name="last_name_father" readonly="1"/>
                <field name="last_name_mother" readonly="1"/>
                <field name="names" readonly="1"/>
                <field name="fecha_ingreso" readonly="1" string="Fecha computo"/>
                <!-- <field name="fecha_computable" /> -->
                <field name="fecha_cese" readonly="1"/>
                <field name="monto"/>
            </tree>
        </field>
    </record>
</odoo>
        