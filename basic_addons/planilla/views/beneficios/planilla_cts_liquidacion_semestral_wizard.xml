<odoo>
    <record model="ir.ui.view" id="planilla_cts_liquidacion_semestral_wizard">
        <field name="name">planilla_cts_liquidacion_semestral_wizard</field>
        <field name="model">planilla.cts.liquidacion.semestral.wizard</field>
        <field name="arch" type="xml">
            <form string="CTS" version="8.0">
                <group>
                    <group>
                        <field name="encargado"/>
                    </group>
                    <group>
                        <field name="fecha_deposito"/>
                    </group>
                </group>
                <footer>
                    <button string="Generar Reporte" type="object" name="get_pdf" class="oe_highlight"/>
                        or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

